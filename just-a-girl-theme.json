{
  "name": "One Dark Pro Flat",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "annotation:dart": {
      "foreground": "#c37c98"
    },
    "enumMember": {
      "foreground": "#de0b8a"
    },
    "macro": {
      "foreground": "#c37c98"
    },
    "memberOperatorOverload": {
      "foreground": "#c90076"
    },
    "parameter.label:dart": {
      "foreground": "#c7ccd6"
    },
    "property:dart": {
      "foreground": "#c37c98"
    },
    "tomlArrayKey": {
      "foreground": "#c37c98"
    },
    "variable:dart": {
      "foreground": "#c37c98"
    },
    "variable.constant": {
      "foreground": "#c37c98"
    },
    "variable.defaultLibrary": {
      "foreground": "#c37c98"
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#c37c98",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#c37c98"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator.delimiter",
        "punctuation.separator.c",
        "punctuation.separator.cpp",
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python",
        "punctuation.separator.arguments.python",
        "punctuation.definition.list.begin.python",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.list.end.python",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#c7ccd6"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#dce1e5"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#c90076",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.require",
        "support.function.any-method",
        "variable.function"
      ],
      "settings": {
        "foreground": "#a3537b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.language",
        "variable.c",
        "import.storage.java",
        "token.variable.parameter.java",
        "keyword.operator.expression.import"
      ],
      "settings": {
        "foreground": "#a62772"
      }
    },
    {
      "name": "Text - Vari√°veis",
      "scope": [
        "variable.parameter.function",
        "invalid.illegal.bad-ampersand.html",
        "meta.embedded",
        "punctuation.separator.period.python",
        "punctuation.separator.element.python",
        "punctuation.parenthesis.begin.python",
        "punctuation.parenthesis.end.python",
        "storage.modifier.lifetime.rust"
      ],
      "settings": {
        "foreground": "#c7ccd6"
      }
    },
    {
      "name": "Single quoted strings",
      "scope": "string.quoted.single",
      "settings": {
        "foreground": "#d8c1d9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Double quoted strings",
      "scope": "string.quoted.double",
      "settings": {
        "foreground": "#d8c1d9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Other quoted strings",
      "scope": "string.quoted.other",
      "settings": {
        "foreground": "#d8c1d9"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#dce1e5"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "string.interpolated",
        "punctuation.definition.string.begin.ts",
        "punctuation.definition.string.end.ts",
        "punctuation.definition.string.begin.tsx",
        "punctuation.definition.string.end.tsx",
        "punctuation.definition.string.begin.jsx",
        "punctuation.definition.string.end.jsx"
      ],
      "settings": {
        "foreground": "#d8c1d9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#dd7bb5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.class.identifier.namespace.type",
        "variable.other.class.js",
        "variable.other.class.php",
        "variable.other.class.ts"
      ],
      "settings": {
        "foreground": "#dd7bb5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Namespaces",
      "scope": "entity.name.namespace",
      "settings": {
        "foreground": "#dd7bb5"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword.operator.expression.instanceof",
        "keyword.operator.new",
        "keyword.operator.ternary",
        "keyword.operator.optional",
        "keyword.operator.delete",
        "keyword.operator.expression.keyof",
        "keyword.operator.instanceof.java",
        "keyword.operator.assignment.c",
        "keyword.operator.comparison.c",
        "keyword.operator.c",
        "keyword.operator.increment.c",
        "keyword.operator.decrement.c",
        "keyword.operator.bitwise.shift.c",
        "keyword.operator.assignment.cpp",
        "keyword.operator.comparison.cpp",
        "keyword.operator.cpp",
        "keyword.operator.increment.cpp",
        "keyword.operator.decrement.cpp",
        "keyword.operator.bitwise.shift.cpp",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.relational",
        "keyword.operator.sigil.rust",
        "keyword.operator.logical.python",
        "keyword.operator.assignment.compound",
        "keyword.operator.error-control.php",
        "keyword.operator.type.php",
        "keyword.operator.regexp.php",
        "keyword.operator.heredoc.php,keyword.operator.nowdoc.php",
        "keyword.operator.module",
        "keyword.operator.arithmetic.go",
        "keyword.operator.address.go",
        "keyword.operator.arithmetic.cpp",
        "keyword.operator.expression.is",
        "keyword.operator.sizeof.c,keyword.operator.sizeof.cpp",
        "keyword.operator.expression.delete,keyword.operator.expression.in"
      ],
      "settings": {
        "foreground": "#c90076"
      }
    },
    {
      "name": "Keywords - italic",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#c90076",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword Operator - bold",
      "scope": [
        "keyword.operator.word",
        "keyword.control",
        "entity.name.class.xi"
      ],
      "settings": {
        "foreground": "#c90076",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#a74e79"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "punctuation.definition.constant"
      ],
      "settings": {
        "foreground": "#fdd9de"
      }
    },
    {
      "name": "Constant other symbol",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#dd7bb5"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#c90076"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#a11969"
      }
    },
    {
      "name": "Classes",
      "scope": "entity.name.type.namespace",
      "settings": {
        "foreground": "#c37c98"
      }
    },
    {
      "name": "Classes",
      "scope": ".support.class, entity.name.type.class",
      "settings": {
        "foreground": "#c90076"
      }
    },
    {
      "name": "Type Name",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#c37c98"
      }
    }
  ]
}
