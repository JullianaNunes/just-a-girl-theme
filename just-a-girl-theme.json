{
  "name": "Just a Girl",
  "type": "dark",
  "semanticHighlighting": true,

  // ========================================
  // Semantic Tokens
  // ========================================
  "semanticTokenColors": {
    "annotation:dart": { "foreground": "#c37c98" },
    "enumMember": { "foreground": "#de0b8a" },
    "macro": { "foreground": "#c37c98" },
    "memberOperatorOverload": { "foreground": "#c90076" },
    "parameter.label:dart": { "foreground": "#c7ccd6" },
    "property:dart": { "foreground": "#c37c98" },
    "tomlArrayKey": { "foreground": "#c37c98" },
    "variable:dart": { "foreground": "#c37c98" },
    "variable.constant": { "foreground": "#c37c98" },
    "variable.defaultLibrary": { "foreground": "#c37c98" }
  },

  // ========================================
  // Token Colors
  // ========================================
  "tokenColors": [
    // ---------------- Comentários ----------------
    {
      "name": "Comentários",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#c37c98", "fontStyle": "italic" }
    },
    {
      "name": "Comentários - XI",
      "scope": "beginning.punctuation.definition.list.markdown.xi",
      "settings": { "foreground": "#f7f8f8" }
    },
    {
      "name": "Comentários - Markup",
      "scope": "comment.markup.link",
      "settings": { "foreground": "#5c6370" }
    },

    // ---------------- Strings ----------------
    { "name": "Strings — Geral", "scope": "string", "settings": { "foreground": "#dce1e5" } },
    { "name": "Strings — Aspas simples", "scope": "string.quoted.single", "settings": { "foreground": "#d8c1d9", "fontStyle": "italic" } },
    { "name": "Strings — Aspas duplas", "scope": "string.quoted.double", "settings": { "foreground": "#d8c1d9", "fontStyle": "italic" } },
    { "name": "Strings — Outras", "scope": "string.quoted.other", "settings": { "foreground": "#d8c1d9" } },
    {
      "name": "Strings — Interpoladas (JS/TS/JSX/TSX)",
      "scope": [
        "string.interpolated",
        "punctuation.definition.string.begin.ts",
        "punctuation.definition.string.end.ts",
        "punctuation.definition.string.begin.tsx",
        "punctuation.definition.string.end.tsx",
        "punctuation.definition.string.begin.jsx",
        "punctuation.definition.string.end.jsx"
      ],
      "settings": { "foreground": "#d8c1d9", "fontStyle": "italic" }
    },
    { "name": "Strings — Caracteres de escape", "scope": "constant.character.escape", "settings": { "foreground": "#dd7bb5", "fontStyle": "bold" } },
    {
      "name": "Strings - JSON",
      "scope": [
        "source.json.meta.structure.dictionary.json",
        "string.quoted.json",
        "source.json.meta.structure.dictionary.json string.quoted.json"
      ],
      "settings": { "foreground": "#bd4b83" }
    },
    {
      "name": "Strings - Markdown",
      "scope": "punctuation.definition.string.markdown",
      "settings": { "foreground": "#c90076" }
    },
    { "name": "Strings - Regexp", "scope": "string.regexp", "settings": { "foreground": "#c90076" } },
    {
      "name": "Strings - Markdown",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "String - LaTeX",
      "scope": "string.other.math.block.environment.latex",
      "settings": { "foreground": "#61afef" }
    },

    // ---------------- Palavras-chave / Operadores ----------------
    { "name": "Palavras-chave — Geral", "scope": ["keyword", "keyword.other.unit"], "settings": { "foreground": "#c90076", "fontStyle": "italic" } },
    { "name": "Palavras-chave — Operadores (bold)", "scope": ["keyword.operator.word", "keyword.control"], "settings": { "foreground": "#c90076", "fontStyle": "bold" } },
    {
      "name": "Operadores — Vários",
      "scope": [
        "keyword.operator.expression.instanceof",
        "keyword.operator.new",
        "keyword.operator.ternary",
        "keyword.operator.optional",
        "keyword.operator.delete",
        "keyword.operator.expression.keyof",
        "keyword.operator.instanceof.java",
        "keyword.operator.assignment.c",
        "keyword.operator.comparison.c",
        "keyword.operator.c",
        "keyword.operator.increment.c",
        "keyword.operator.decrement.c",
        "keyword.operator.bitwise.shift.c",
        "keyword.operator.assignment.cpp",
        "keyword.operator.comparison.cpp",
        "keyword.operator.cpp",
        "keyword.operator.increment.cpp",
        "keyword.operator.decrement.cpp",
        "keyword.operator.bitwise.shift.cpp",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.relational",
        "keyword.operator.sigil.rust",
        "keyword.operator.logical.python",
        "keyword.operator.assignment.compound",
        "keyword.operator.error-control.php",
        "keyword.operator.type.php",
        "keyword.operator.regexp.php",
        "keyword.operator.heredoc.php,keyword.operator.nowdoc.php",
        "keyword.operator.module",
        "keyword.operator.arithmetic.go",
        "keyword.operator.address.go",
        "keyword.operator.arithmetic.cpp",
        "keyword.operator.expression.is",
        "keyword.operator.sizeof.c,keyword.operator.sizeof.cpp",
        "keyword.operator.expression.delete,keyword.operator.expression.in"
      ],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Operadores",
      "scope": [
        "keyword.operator.logical",
        "keyword.operator.bitwise",
        "keyword.operator.assignment.compound.js",
        "keyword.operator.assignment.compound.ts",
        "keyword.operator.assignment.scss",
        "keyword.operator.less"
      ],
      "settings": { "foreground": "#de0b8a" }
    },
    {
      "name": "Operadores - PHP/Rust",
      "scope": [
        "keyword.operator.misc.rust",
        "keyword.operator.assignment.php"
      ],
      "settings": { "foreground": "#c7ccd6" }
    },
    {
      "name": "Operadores - Python",
      "scope": "keyword.operator.comparison.python",
      "settings": { "foreground": "#de0b8a" }
    },
    {
      "name": "Operadores - PHP/JSON",
      "scope": [
        "keyword.operator.logical.php",
        "keyword.operator.bitwise.php",
        "keyword.operator.arithmetic.php",
        "source.json.meta.structure.dictionary.json",
        "source.json.meta.structure.array.json",
        "constant.language.json"
      ],
      "settings": { "foreground": "#de0b8a" }
    },
    {
      "name": "Operador - Atribuição",
      "scope": "keyword.operator.assignment",
      "settings": { "foreground": "#de0b8a" }
    },

    // ---------------- Variáveis ----------------
    { "name": "Variáveis — Geral", "scope": ["variable", "variable.language", "variable.c", "token.variable.parameter.java"], "settings": { "foreground": "#a62772" } },
    { "name": "Parâmetros de função", "scope": "variable.parameter.function", "settings": { "foreground": "#c7ccd6" } },
    { "name": "Variáveis SQL", "scope": ["text.variable", "text.bracketed"], "settings": { "foreground": "#c90076" } },
    { "name": "Variáveis - Local", "scope": "entity.name.variable.local.cs", "settings": { "foreground": "#c90076" } },
    { "name": "Variáveis - Outros", "scope": ["variable.other.generic-type.haskell", "variable.other.readwrite.c"], "settings": { "foreground": "#c90076" } },
    { "name": "Variáveis - PHP", "scope": "entity.name.variable.php", "settings": { "foreground": "#61afef" } },
    { "name": "Variável - Constante", "scope": "variable.other.constant.property", "settings": { "foreground": "#c90076" } },
    { "name": "Variável - Lambda", "scope": "variable.other.readwrite", "settings": { "foreground": "#c90076" } },
    { "name": "Variável - Propriedade", "scope": "variable.other.property", "settings": { "foreground": "#c90076" } },

    // ---------------- Funções ----------------
    { "name": "Funções", "scope": ["entity.name.function", "support.function.any-method", "variable.function"], "settings": { "foreground": "#a3537b", "fontStyle": "bold" } },
    { "name": "Métodos", "scope": "keyword.other.special-method", "settings": { "foreground": "#de0b8a" } },
    { "name": "Função de suporte", "scope": "support.function.std.rust", "settings": { "foreground": "#61afef" } },
    { "name": "Suporte a função", "scope": "support.function", "settings": { "foreground": "#de0b8a" } },
    {
      "name": "Função - Outros",
      "scope": [
        "meta.function-call.php",
        "meta.function-call.object.php",
        "meta.function-call.static.php"
      ],
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "Funções - LaTeX",
      "scope": [
        "support.function.latex",
        "support.function.latex.math.environment"
      ],
      "settings": { "foreground": "#c90076" }
    },

    // ---------------- Constantes / Números ----------------
    { "name": "Números (inteiros/decimal)", "scope": "constant.numeric", "settings": { "foreground": "#a74e79" } },
    { "name": "Constantes — Geral", "scope": ["constant", "punctuation.definition.constant"], "settings": { "foreground": "#fdd9de" } },
    { "name": "Constantes — Símbolos", "scope": "constant.other.symbol", "settings": { "foreground": "#dd7bb5" } },
    { "name": "Suporte de constante", "scope": "support.constant.font-name", "settings": { "foreground": "#c37c98" } },
    { "name": "Suporte de constante (property-value)", "scope": "support.constant.property-value", "settings": { "foreground": "#c7ccd6" } },
    {
      "name": "Constantes - LaTeX",
      "scope": "constant.other.general.math.latex",
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "Constantes - PHP",
      "scope": "constant.language.php",
      "settings": { "foreground": "#de0b8a" }
    },
    {
      "name": "Constantes - Ruby",
      "scope": "constant.language.symbol.ruby",
      "settings": { "foreground": "#de0b8a" }
    },
    {
      "name": "Constantes - Elixir",
      "scope": [
        "constant.language.symbol.elixir",
        "constant.language.symbol.double-quoted.elixir"
      ],
      "settings": { "foreground": "#de0b8a" }
    },
    {
      "name": "Constantes - Outros",
      "scope": [
        "constant.character.xi",
        "constant.character-class.regexp.xi",
        "constant.regexp.xi"
      ],
      "settings": { "foreground": "#c90076" }
    },

    // ---------------- Classes / Tipos / Namespaces ----------------
    { "name": "Classes — Nome", "scope": ["entity.name.class", "entity.name.class.identifier.namespace.type", "variable.other.class.js", "variable.other.class.php", "variable.other.class.ts"], "settings": { "foreground": "#dd7bb5", "fontStyle": "bold" } },
    { "name": "Classes — Nome (xi)", "scope": "entity.name.class.xi", "settings": { "foreground": "#c90076", "fontStyle": "bold" } },
    { "name": "Namespaces", "scope": "entity.name.namespace", "settings": { "foreground": "#dd7bb5" } },
    { "name": "Tipos — Nome", "scope": "entity.name.type", "settings": { "foreground": "#c37c98" } },
    { "name": "Tipos — Namespace", "scope": "entity.name.type.namespace", "settings": { "foreground": "#c37c98" } },
    { "name": "Classes — Suporte/Type", "scope": "support.class, entity.name.type.class", "settings": { "foreground": "#c90076" } },
    {
      "name": "Tipo de Suporte",
      "scope": [
        "support.type.property-name",
        "support.constant.property-value"
      ],
      "settings": { "foreground": "#c7ccd6" }
    },
    {
      "name": "Suporte de armazenamento",
      "scope": [
        "token.storage",
        "token.storage.type.java"
      ],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Pacotes",
      "scope": "token.package.keyword",
      "settings": {
        "foreground": "#22d2cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Pacotes",
      "scope": "token.package.keyword",
      "settings": { "foreground": "#c7ccd6" }
    },
    {
      "name": "Suporte - Tipos",
      "scope": [
        "support.type.posix-reserved.c",
        "support.type.posix-reserved.cpp",
        "support.type.python"
      ],
      "settings": { "foreground": "#de0b8a" }
    },
    {
      "name": "Suporte - Tipos (Console/Processo)",
      "scope": [
        "support.type.object.console",
        "support.variable.property.process"
      ],
      "settings": { "foreground": "#a11266" }
    },
    {
      "name": "Suporte - Tipos (Objetos)",
      "scope": "support.type.object",
      "settings": { "foreground": "#de0b8a" }
    },
    {
      "name": "Suporte - Tipos (Outros)",
      "scope": [
        "support.type.flowtype",
        "support.type.property-name.json",
        "support.type.sampler.hlsl",
        "support.type.object.hlsl",
        "support.type.fx.hlsl",
        "support.type.object.hlsl"
      ],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Suporte - Tipos (Markup)",
      "scope": "support.type.texture.hlsl",
      "settings": { "foreground": "#c90076" }
    },

    // ---------------- HTML / CSS ----------------
    { "name": "HTML — Tags (entity.name.tag)", "scope": "entity.name.tag", "settings": { "foreground": "#c90076" } },
    { "name": "HTML/CSS — Atributos", "scope": ["entity.other.attribute-name", "entity.other.attribute-name.class.css"], "settings": { "foreground": "#a11969" } },
    { "name": "Markup — Meta de tag (meta.tag)", "scope": "meta.tag", "settings": { "foreground": "#c7ccd6" } },
    {
      "name": "IDs de Atributo e Títulos",
      "scope": [
        "entity.other.attribute-name.id",
        "markup.heading.punctuation.definition.heading",
        "entity.name.section",
        "support.function.std.rust"
      ],
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "Seletor",
      "scope": "meta.selector",
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Markup - Heading",
      "scope": "markup.heading",
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Markup - Outros",
      "scope": [
        "markup.italic",
        "markup.underline.link",
        "markup.underline.link.image.markdown",
        "markup.link.css",
        "markup.other.url.asciidoc",
        "markup.quote.markdown"
      ],
      "settings": { "foreground": "#c90076" }
    },
    { "name": "Markup - Deleted", "scope": "markup.deleted", "settings": { "foreground": "#c90076" } },

    // ---------------- Pontuação ----------------
    {
      "name": "Pontuação — Delimitadores/Listas/Parênteses",
      "scope": [
        "punctuation.separator.delimiter",
        "punctuation.separator.c",
        "punctuation.separator.cpp",
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python",
        "punctuation.separator.arguments.python",
        "punctuation.definition.list.begin.python",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.list.end.python",
        "punctuation.separator.period.python",
        "punctuation.separator.element.python",
        "punctuation.parenthesis.begin.python",
        "punctuation.parenthesis.end.python"
      ],
      "settings": { "foreground": "#c7ccd6" }
    },
    {
      "name": "Pontuação - Outros",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "punctuation.definition.heading.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.quasi.element"
      ],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Pontuação - XML/YAML",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.string.begin.xml",
        "punctuation.definition.string.end.xml",
        "punctuation.definition.list.begin.yaml",
        "punctuation.definition.list.end.yaml",
        "punctuation.definition.sequence.item.yaml"
      ],
      "settings": { "foreground": "#c7ccd6" }
    },
    {
      "name": "Pontuação - PHP",
      "scope": [
        "punctuation.section.block.begin.php",
        "punctuation.section.block.end.php",
        "punctuation.terminator.statement.c.php",
        "punctuation.separator.list.comma.css",
        "punctuation.section.array.end.php",
        "punctuation.section.array.begin.php",
        "punctuation.definition.parameters.begin.bracket.round.php",
        "punctuation.definition.parameters.end.bracket.round.php",
        "punctuation.separator.delimiter.php",
        "punctuation.section.block.begin.java",
        "punctuation.section.block.end.java",
        "punctuation.definition.method-parameters.begin.java",
        "punctuation.definition.method-parameters.end.java"
      ],
      "settings": { "foreground": "#c7ccd6" }
    },
    {
      "name": "Pontuação - Outros",
      "scope": [
        "punctuation.definition.italic",
        "punctuation.definition.list.begin.unison",
        "punctuation.definition.list.end.unison",
        "punctuation.definition.ability.begin.unison",
        "punctuation.definition.ability.end.unison",
        "punctuation.operator.assignment.as.unison",
        "punctuation.separator.pipe.unison",
        "punctuation.separator.delimiter.unison",
        "punctuation.definition.hash.unison"
      ],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Pontuação - PHP",
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": { "foreground": "#be5046" }
    },
    {
      "name": "Pontuação - PHP",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": { "foreground": "#f6f4f1" }
    },
    {
      "name": "Pontuação - Outros",
      "scope": [
        "punctuation.definition.interpolation",
        "punctuation.definition.delayed.unison"
      ],
      "settings": { "foreground": "#c90076" }
    },

    // ---------------- Linguagens específicas ----------------
    { "name": "Java — import (storage)", "scope": "import.storage.java", "settings": { "foreground": "#a62772" } },
    { "name": "Operador — import (JS/TS)", "scope": "keyword.operator.expression.import", "settings": { "foreground": "#a62772" } },
    { "name": "Rust — lifetime (storage.modifier)", "scope": "storage.modifier.lifetime.rust", "settings": { "foreground": "#c7ccd6" } },
    {
      "name": "Markdown - Meta",
      "scope": [
        "meta.embedded.block.markdown",
        "meta.embedded.line.markdown",
        "source.php.embedded.line.html",
        "text.html.laravel-blade.source.php.embedded.line.html"
      ],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Token - Markdown",
      "scope": [
        "meta.paragraph.markdown",
        "entity.name.tag.laravel-blade"
      ],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Source - Rust/Elixir",
      "scope": ["source.rust", "source.elixir"],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "LaTeX",
      "scope": ["variable.parameter.function.latex", "constant.other.readwrite.c"],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Símbolos - Clojure",
      "scope": "meta.symbol.clojure",
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "PHP",
      "scope": ["support.other.php", "entity.name.goto-label.php"],
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "PHP - Diff",
      "scope": [
        "meta.diff.header.from-file.diff",
        "meta.diff.header.to-file.diff",
        "punctuation.definition.from-file.diff",
        "punctuation.definition.to-file.diff"
      ],
      "settings": { "foreground": "#61afef" }
    },

    // ---------------- Meta ----------------
    { "name": "Meta — Código embutido", "scope": "meta.embedded", "settings": { "foreground": "#c7ccd6" } },
    { "name": "Meta — require (Node/JS)", "scope": "meta.require", "settings": { "foreground": "#a3537b", "fontStyle": "bold" } },
    { "name": "Underline", "scope": "markup.underline", "settings": { "fontStyle": "underline" } },
    { "name": "Elementos de Controle", "scope": ["control.elements", "keyword.operator.less"], "settings": { "foreground": "#c37c98" } },
    { "name": "Expressões Regulares", "scope": "string.regexp", "settings": { "foreground": "#de0b8a" } },
    {
      "name": "Meta - Títulos",
      "scope": [
        "meta.di.dtl.header.to-file",
        "meta.di.dtl.header.to-file.di",
        "punctuation.definition.from-file.di",
        "punctuation.definition.to-file.di"
      ],
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "Meta - Outros",
      "scope": [
        "meta.template.expression",
        "meta.template.expression"
      ],
      "settings": { "foreground": "#c7ccd6" }
    },
    {
      "name": "Meta - Atributo",
      "scope": [
        "meta.object.literal.key",
        "support.variable.property.object",
        "support.variable.property.process",
        "support.variable.object.process",
        "support.variable.object.toml",
        "support.type.property-name.toml",
        "support.type.property-name.table.toml",
        "support.type.property-name.array.toml"
      ],
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Meta - Variável",
      "scope": "meta.definition.variable.groovy",
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Meta - Go",
      "scope": "meta.function-call.go",
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Token - Outros",
      "scope": ["token.info-token", "token.debug-token"],
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "Meta - Markup",
      "scope": "meta.tag.other.html",
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Meta - Markup",
      "scope": "markup.underline.link",
      "settings": { "foreground": "#c90076" }
    },
    {
      "name": "Outros",
      "scope": [
        "support.variable.property.dom",
        "constant.language.clojure",
        "constant.language.python",
        "support.type.prelude.elm"
      ],
      "settings": { "foreground": "#de0b8a" }
